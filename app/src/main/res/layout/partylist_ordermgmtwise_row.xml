<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/fdrRelativeLayout"
    android:background="?android:attr/activatedBackgroundIndicator"
    android:padding="2sp"
    >

    <LinearLayout
        android:id="@+id/Layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:background="#FFF"
        android:orientation="horizontal">
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow>
                <TextView
                    style="@style/newtextLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="Party Name"
                    />
                <TextView
                    android:id="@+id/mobileNoOnOrderMgmtRow"
                    style="@style/newtextLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="Area Name"
                    />
            </TableRow>
            <TableRow
                android:background="@color/white">
                <TextView
                    android:id="@+id/partyNameOnOrderMgmtRow"
                    style="@style/neweditLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"

                    />

                <TextView
                    android:id="@+id/beatNameViewOnOrderMgmtRow"
                    style="@style/neweditLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"

                    />
            </TableRow>

            <TableRow
                >
                <TextView
                    android:id="@+id/textViewremark"
                    style="@style/newtextLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="Beat"
                    />

                <TextView
                    android:id="@+id/textViewremark"
                    style="@style/newtextLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="Qty"
                    />
            </TableRow>
            <TableRow
                android:background="@color/white"
                >
                <TextView
                    android:id="@+id/beatName"
                    style="@style/neweditLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"

                    />
                <TextView
                    android:id="@+id/qty"
                    style="@style/neweditLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"

                    />
            </TableRow>

            <TableRow
                android:background="@color/white">
                <TextView
                    android:id="@+id/textView1"
                    style="@style/newtextLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="OrderAmount:"
                    />
            </TableRow>
            <TableRow
                android:background="@color/white">
                <TextView
                    android:id="@+id/orderAmount"
                    style="@style/neweditLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"


                    />
            </TableRow>

            <TableRow
                android:background="@color/white">
                <TextView
                    android:id="@+id/remarkLabel"
                    style="@style/newtextLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="Remark:"
                    />
            </TableRow>
            <TableRow
                android:background="@color/white">
                <TextView
                    android:id="@+id/remark"
                    style="@style/neweditLebal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"


                    />
            </TableRow>
            <TableRow
                android:gravity="center"
                android:background="@color/white">
                <Button
                    android:id="@+id/button"
                    android:text="Detail"
                    android:textAllCaps="false"
                    android:layout_alignParentTop="true"
                    android:layout_margin="@dimen/button_margin"
                    android:layout_height="@dimen/pendinsyn_button_height"
                    android:layout_width="match_parent"
                    android:background="@drawable/button_style"
                    android:textColor="#ffffff"
                    android:textStyle="bold"/>
            </TableRow>
        </TableLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#000"
        android:layout_below="@+id/Layout"></LinearLayout>
</RelativeLayout>
